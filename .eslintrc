{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript"
  ],
  "plugins": [
    "prettier",
    "import",
    "react",
    "react-hooks",
    "sort-destructure-keys",
    "sort-keys-fix"
  ],
  "root": true,
  "env": {
    "browser": true,
    "amd": true,
    "node": true
  },
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": "off",
    "import/default": ["off"],
    "import/extensions": [
      "error",
      {
        "js": "never",
        "json": "always",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ],
    "import/newline-after-import": ["error"],
    "import/no-duplicates": ["error"],
    "import/no-named-as-default-member": ["off"],
    "import/no-unresolved": ["error"],
    "import/no-unused-modules": ["error"],
    "import/order": [
      "error",
      {
        "alphabetize": {
          "caseInsensitive": false,
          "order": "asc"
        },
        "groups": ["builtin", "external", "parent", "internal", "index"],
        "newlines-between": "always"
      }
    ],
    "no-console": ["error"],
    "no-eval": ["error", { "allowIndirect": true }],
    "no-implied-eval": ["error"],
    "no-nested-ternary": ["warn"],
    "no-shadow": ["off"],
    "no-unneeded-ternary": ["error"],
    "no-var": ["error"],
    "padding-line-between-statements": [
      "error",
      { "blankLine": "always", "next": "return", "prev": "*" },
      { "blankLine": "always", "next": "throw", "prev": "*" },
      { "blankLine": "always", "next": "if", "prev": "*" },
      { "blankLine": "always", "next": "*", "prev": "if" },
      { "blankLine": "always", "next": "for", "prev": "*" },
      { "blankLine": "always", "next": "*", "prev": "for" },
      { "blankLine": "always", "next": "while", "prev": "*" },
      { "blankLine": "always", "next": "*", "prev": "while" },
      { "blankLine": "always", "next": "try", "prev": "*" },
      { "blankLine": "always", "next": "*", "prev": "try" },
      { "blankLine": "always", "next": "switch", "prev": "*" },
      { "blankLine": "always", "next": "*", "prev": "switch" },
      { "blankLine": "always", "next": "class", "prev": "*" },
      { "blankLine": "always", "next": "*", "prev": "class" }
    ],
    "prefer-const": ["error"],
    "prettier/prettier": ["error", { "endOfLine": "auto" }],
    "react-hooks/rules-of-hooks": ["error"],
    "react/function-component-definition": [
      "error",
      {
        "namedComponents": "arrow-function",
        "unnamedComponents": "arrow-function"
      }
    ],
    "react/jsx-boolean-value": ["error"],
    "react/jsx-filename-extension": ["error", { "extensions": [".tsx"] }],
    "react/jsx-key": ["error"],
    "react/jsx-no-useless-fragment": ["error"],
    "react/jsx-pascal-case": ["error"],
    "react/jsx-sort-props": [
      "warn",
      {
        "ignoreCase": false,
        "noSortAlphabetically": false,
        "reservedFirst": ["key"]
      }
    ],
    "react/prefer-stateless-function": ["error"],
    "react/prop-types": ["off"],
    "require-await": ["error"],
    "sort-destructure-keys/sort-destructure-keys": ["error"],
    "sort-imports": [
      "error",
      {
        "ignoreDeclarationSort": true,
        "ignoreMemberSort": false
      }
    ],
    "sort-keys": [
      "error",
      "asc",
      { "caseSensitive": true, "minKeys": 2, "natural": false }
    ],
    "sort-keys-fix/sort-keys-fix": ["error"]
  },
  "settings": {
    "import/extensions": [".js", ".jsx", ".ts", ".tsx"],
    "import/ignore": ["react"],
    "import/parsers": {
      "@typescript-eslint/parser": [".ts", ".tsx"]
    },
    "import/resolver": {
      "typescript": {"alwaysTryTypes": true}
    }
  }
}